!  Colour Scheme

*background: #1D2426
*foreground: #5f767c
!*foreground: #8FA388
*cursorColor: #513e4d

!black
*color0:  #39474A
*color8:  #4C5F63
!red
*color1:  #a54242
*color9:  #cc6666
!green
*color2:  #315c2b
*color10: #7fb069
!yellow
*color3:  #C89A7B
*color11: #BA9378
!blue
*color4:  #313b72
*color12: #81a2be
!magenta
*color5:  #85678f
*color13: #b294bb
!cyan
*color6:  #598392
*color14: #979dbf
!white
*color7:  #707880
*color15: #c5c8c6


!  Style

! Set the scroll bar: rxvt, plain, next, xterm
URxvt.scrollstyle: rxvt
! Disabled
URxvt.scrollBar: false


!  Font

URxvt*font: xft:Inconsolata:size=8, xft:wqy-zenhei
Xft*dpi:       96
Xft*antialias: True
Xft*hinting:   Full


!  Perl Extensions

URxvt.perl-ext-common:	default,clipboard,matcher,keyboard-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-launcher:	/usr/bin/firefox
URxvt.underlineURLs:	True
URxvt.matcher.button:	1
URxvt.keysym.M-Escape:	perl:keyboard-select:active
URxvt.keysym.M-c:	perl:clipboard:copy
URxvt.keysym.M-v:	perl:clipboard:paste
URxvt.keysym.M-C-v:	perl:clipboard:paste_escaped
URxvt.clipboard.autocopy: true
